{
  "name": "@digdir/einnsyn-sdk",
  "version": "1.0.0",
  "description": "TypeScript client library for the eInnsyn API",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.15.29",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "minify": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "homepage": "https://github.com/felleslosninger/einnsyn-sdk-typescript",
  "repository": {
    "type": "git",
    "url": "git://github.com/felleslosninger/einnsyn-sdk-typescript.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "BSD-3-Clause"
}
